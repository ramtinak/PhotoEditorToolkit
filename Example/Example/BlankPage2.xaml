<Page
    x:Class="Example.BlankPage2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:editor="using:PhotoEditorToolkit.Controls"
    xmlns:local="using:Example"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    mc:Ignorable="d">

    <Grid>
        <editor:PhotoEditor x:Name="Editor" />

        <Button
            x:Name="AccepscdtButton"
            Margin="0"
            Click="AccepscdtButton_Click"
            Content="&#xE10B;"
            FontSize="30" />
        <Button
            x:Name="AccepscdtBussstton"
            Margin="0"
            HorizontalAlignment="Right"
            Click="AccepscdtBussstton_Click"
            Content="&#xE10B;"
            FontSize="30" />
    </Grid>
</Page>
